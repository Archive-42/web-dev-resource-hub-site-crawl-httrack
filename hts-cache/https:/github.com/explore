            <a data-hydro-click="{&quot;event_type&quot;:&quot;explore.click&quot;,&quot;payload&quot;:{&quot;click_context&quot;:&quot;MARKETPLACE_APP_CARD&quot;,&quot;click_target&quot;:&quot;MARKETPLACE_APP&quot;,&quot;click_visual_representation&quot;:&quot;MARKETPLACE_APP_TITLE&quot;,&quot;actor_id&quot;:null,&quot;record_id&quot;:1128,&quot;originating_url&quot;:&quot;https://github.com/explore&quot;,&quot;user_id&quot;:null}}" data-hydro-click-hmac="29c632c564dc509daa554d2baf1c2db1158c12f0e98bf66d797f4134bb860dc7" data-ga-click="Explore, go to marketplace app, location:explore feed; text:Depfu" href="/marketplace/depfu">
