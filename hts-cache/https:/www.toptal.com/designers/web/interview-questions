var anonymizeIp = false
var gaContentGroup = (window.gon && window.gon.ga_settings) ? window.gon.ga_settings.group : undefined

//Leave it available globally to be initialized when the google analytics widgets gets loaded
window.googleAnalyticsTrackingId = trackingId

window.dataLayer = window.dataLayer || []
window.gtag = function () { dataLayer.push(arguments) }

gtag('js', new Date())
gtag('config', trackingId, Object.assign(
  {
    'anonymize_ip': anonymizeIp,
    'content_group1': gaContentGroup,
    'linker': ['www.toptal.com', 'staging.toptal.net'],
    'custom_map': {
      'dimension10': 'client_id'
    }
  },
  (window.gon && gon.chameleon_current_experiments) ? buildChameleonExperiments() : {},
  buildAudienceConfig(),
  buildDebugConfig()
))

track15SecondsRead()
addBlogPageVisitToDataLayer()

/*
* Parses Chameleon experiments and adds each applicable one to Google Analytics’ config.
*
* List of experiments comes from gon as an object.
* We don't use the keys of this object, we destructure assign each object's value.
*
* An example of a gon.chameleon_current_experiments:
*
*  {
*    new_global_nav: {google_analytics_dimension: 'dimension12', variant_key: 'disabled'},
*    some_page: {google_analytics_dimension: null, variant_key: 'enabled'}
*  }
*
*  Because `some_page` has `null` for its `google_analytics_dimension`, it will be ignored.
*
*  The result will be:
*
*  {'dimension12': 'disabled'}
*
*/
function buildChameleonExperiments () {
  return Object.entries(gon.chameleon_current_experiments).reduce((acc, experiment) => {
    const {google_analytics_dimension, variant_key} = experiment[1]

    if (google_analytics_dimension !== null) {
      let configObject = {}
      configObject[google_analytics_dimension] = variant_key

      return Object.assign(acc, configObject)
    }
    return acc
  }, {})
}

function buildAudienceConfig() {
  const urlParams = new URLSearchParams(window.location.search)
  const dimension9 = urlParams.get('utm_audience')

  return { ...(dimension9 && { dimension9 }) }
}

// Dimension 22 = Blackfish Code Version
function buildDebugConfig() {
  const dimension22 = '978cf959f47d91c32d1709fc003f7f187554408f'

  return { ...(dimension22 && { dimension22 }) }
}

function track15SecondsRead(){
  setTimeout(() => {
    window.gtag('event', 'read', {'event_category': '15_seconds'})
  }, 15000)
}

function addBlogPageVisitToDataLayer() {
  if (gaContentGroup === "Blog posts") {
    window.gtag('event', 'blogPageVisit',  {
      'event_label': 'Blog Page Visit',
      'event_category': 'blog_page_visit',
      'non_interaction': true
    })
  }
}

function setupGAeventsForCTAsOnRemoteWorkConsultingPage() {
  if (window.location.pathname == '/services/remote-work-consulting-about') {
    function setupListeners() {
      [].slice.call(document.querySelectorAll('[data-ga-action]')).map(function(el) {
        el.addEventListener('click', trackClick.bind(this, el.dataset.gaAction, el.dataset.gaLabel));
      });
    }

    function trackClick(action, label, event) {
      if (window.googleAnalytics != null) {
        window.googleAnalytics.trackRemoteWorkConsulting(action, label)
      }
    }

    document.addEventListener('DOMContentLoaded', setupListeners);
  }
}

setupGAeventsForCTAsOnRemoteWorkConsultingPage();</script><link href="https://bs-assets.toptal.io/blackfish-assets/public/base/images/favicons/toptal-blog-favicon32x32_c2c2ba.png" rel="icon" /><!--[if IE]><link href="https://bs-assets.toptal.io/blackfish-assets/public/base/images/favicons/toptal-blog-favicon32x32_fd5e46.ico" rel="shortcut icon" /><![endif]--><link href="https://bs-assets.toptal.io/blackfish-assets/public/base/images/favicons/toptal-blog-favicon57x57_2ec58c.png" rel="apple-touch-icon-precomposed" sizes="57x57" /><link href="https://bs-assets.toptal.io/blackfish-assets/public/base/images/favicons/toptal-blog-favicon72x72_7485f3.png" rel="apple-touch-icon-precomposed" sizes="72x72" /><link href="https://bs-assets.toptal.io/blackfish-assets/public/base/images/favicons/toptal-blog-favicon114x114_01eb8c.png" rel="apple-touch-icon-precomposed" sizes="114x114" /><link href="https://bs-assets.toptal.io/blackfish-assets/public/base/images/favicons/toptal-blog-favicon57x57_2ec58c.png" rel="apple-touch-icon-precomposed" /><script>
